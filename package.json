{
  "name": "as-pedersen",
  "version": "0.2.1",
  "description": "A pedersen hash implementation in AssemblyScript",
  "main": "index.js",
  "ascMain": "assembly/index.ts",
  "types": "assembly/index.ts",
  "scripts": {
    "test": "asp --verbose",
    "build:debug": "asc assembly/index.ts --target debug",
    "build:release": "asc assembly/index.ts --target release",
    "build": "npm run build:debug && npm run build:release",
    "format": "prettier --write \"assembly/**/*.ts\""
  },
  "keywords": [
    "assemblyscript",
    "pedersen",
    "hash"
  ],
  "license": "MIT",
  "devDependencies": {
    "@as-pect/cli": "6.2.4",
    "assemblyscript": "0.19.10",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  }
}
